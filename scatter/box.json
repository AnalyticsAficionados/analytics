{
	"id": "com.fd.scatter",
	"version": "1.0.0",
	"name": "3DScatter",
	"description": "A 3DScatter SAC Custom Widget",
	"newInstancePrefix": "Scatter",
	"icon": "https://ferrygun.github.io/proj/scatter/box/icon.png",
	"vendor": "FD",
	"eula": "EULA",
	"license": "2.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-fd-scatter",
			"url": "https://ferrygun.github.io/proj/scatter/box/box.js",
			"integrity": "",
			"ignoreIntegrity": true
		},{
			"kind": "builder",
			"tag": "com-fd-scatter-bps",
			"url": "https://ferrygun.github.io/proj/scatter/box/box_bps.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"value": {
			"type": "string",
			"description": "3DScatter value",
			"default": ""
		},
		"title": {
			"type": "string",
			"description": "3DScatter title",
			"default": ""
		},
		"subtitle": {
			"type": "string",
			"description": "3DScatter subtitle",
			"default": ""
		},
		"min": {
			"type": "integer",
			"description": "3DScatter min",
			"default": 0
		},
		"max": {
			"type": "integer",
			"description": "3DScatter max",
			"default": 10
		},
		"width": {
			"type": "integer",
			"default": 50
		},
		"height": {
			"type": "integer",
			"default": 50
		}
	},
	"methods": {
		"setText": {
			"description": "Sets the 3DScatter value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "string",
					"description": "3DScatter Value"
				}
			],
			"body": "this.value = newValue;"
		},
		"getText": {
			"returnType": "string",
			"description": "Returns the 3DScatter value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}
