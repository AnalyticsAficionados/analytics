{
	"id": "com.fd.googlegauge",
	"version": "1.0.0",
	"name": "Google Gauge",
	"description": "A Google Gauge SAC Custom Widget",
	"newInstancePrefix": "GoogleGauge",
	"icon": "ttps://ferrygun.github.io/proj/googlegauge/box/icon.png",
	"vendor": "Pfizer",
	"eula": "EULA",
	"license": "2.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-fd-googlegauge",
			"url": "https://ferrygun.github.io/proj/googlegauge/box/box.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"value": {
			"type": "string",
			"description": "Gauge Value",
			"default": "0:fd"
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"height": {
			"type": "integer",
			"default": 100
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the Gauge Value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "string",
					"description": "Gauge Value"
				}
			],
			"body": "this.value = newValue;"
		},
		"getValue": {
			"returnType": "number",
			"description": "Returns the Gauge value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
}
